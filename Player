using UnityEngine;

public class Player : MonoBehaviour
{
    private int _healh = 100;
    
    public int Healh => _healh;
    
   private void TakeDamage()
   {
        int damage = 10;
        _healh -= damage;
    }

   private void Healed()
   {
       int healed = 10;
       _healh += healed;
   }
}
